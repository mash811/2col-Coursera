{
  "name": "__MSG_Name__",
  "author": "mash",
  "description": "__MSG_Description__",
  "version": "1.0.1",
  "manifest_version": 2,
  "default_locale": "en",
  "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
        "19": "icons/icon19.png"
    },
    "default_title": "2col-Coursera"
  },
  "web_accessible_resources": ["*"],
  "permissions": ["storage"],
  "content_scripts": [
       {
         "matches": ["https://www.coursera.org/learn/*"],
         "css": ["css/style.css"]
       }
  ]
}
